
# Production Environment Variables for SponsrAI
# Domain: www.sponsrai.se
# Server IP: 149.248.202.188

# Supabase Configuration (REQUIRED)
REACT_APP_SUPABASE_URL=your_production_supabase_url
REACT_APP_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# API Configuration
REACT_APP_API_URL=https://www.sponsrai.se/api
REACT_APP_ENVIRONMENT=production

# Analytics (Optional but recommended)
REACT_APP_GA_TRACKING_ID=your_google_analytics_id
REACT_APP_MIXPANEL_TOKEN=your_mixpanel_token

# Email Service (EmailJS)
REACT_APP_EMAILJS_SERVICE_ID=your_emailjs_service_id
REACT_APP_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
REACT_APP_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_TRACKING=true
REACT_APP_ENABLE_CHAT_SUPPORT=true

# Security
REACT_APP_ENABLE_HTTPS=true
REACT_APP_ENABLE_HSTS=true
